{
    "name": "mu/news-module",
    "version": "1.1.0",
    "description": "MUNewsModule module generated by ModuleStudio 1.0.1.",
    "type": "zikula-module",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Michael Ueberschaer",
            "email": "info@homepages-mit-zikula.de",
            "homepage": "https://homepages-mit-zikula.de",
            "role": "owner"
        }
    ],
    "autoload": {
        "psr-4": { "MU\\NewsModule\\": "" }
    },
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "jakub-onderka/php-parallel-lint": "*",
        "phploc/phploc": "*",
        "pdepend/pdepend": "2.*",
        "squizlabs/php_codesniffer": "1.*",
        "phpunit/phpunit": "4.*",
        "theseer/phpdox": "*",
        "phpmd/phpmd": "2.*",
        "sebastian/phpcpd": "*"
    },
    "extra": {
        "zikula": {
            "core-compatibility": ">=1.5.0 <3.0",
            "class": "MU\\NewsModule\\MUNewsModule",
            "displayname": "News",
            "url": "news",
            "oldnames": ["MUNews"],
            "capabilities": {
                "user": {"route": "munewsmodule_message_index"},
                "admin": {"route": "munewsmodule_message_adminindex"},
                "categorizable": {
                    "entities": [
                        "MU\\NewsModule\\Entity\\MessageEntity"
                    ]
                }
            },
            "securityschema": {
                "MUNewsModule::": "::",
                "MUNewsModule:ItemListBlock:": "Block title::",
                "MUNewsModule:ModerationBlock:": "Block title::",
                "MUNewsModule:Message:": "Message ID::",
                "MUNewsModule::Ajax": "::"
            }
        }
    }
}
