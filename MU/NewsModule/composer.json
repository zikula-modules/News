{
	"name" : "mu/news-module",
	"version" : "1.2.5",
	"description" : "MUNewsModule generated by ModuleStudio 1.4.0.",
	"type" : "zikula-module",
	"license" : "LGPL-3.0-or-later",
	"authors" : [{
			"name" : "Michael Ueberschaer",
			"email" : "info@homepages-mit-zikula.de",
			"homepage" : "https://homepages-mit-zikula.de",
			"role" : "owner"
		}
	],
	"autoload" : {
		"psr-4" : {
			"MU\\NewsModule\\" : ""
		}
	},
	"require" : {
		"php" : ">=5.5.9",
		"dompdf/dompdf" : "*"
	},
	"require-dev" : {
		"jakub-onderka/php-parallel-lint" : "*",
		"phploc/phploc" : "*",
		"pdepend/pdepend" : "2.*",
		"squizlabs/php_codesniffer" : "*",
		"phpunit/phpunit" : "4.*",
		"theseer/phpdox" : "*",
		"phpmd/phpmd" : "2.*",
		"sebastian/phpcpd" : "*"
	},
	"extra" : {
		"zikula" : {
			"core-compatibility" : ">=2.0.11 <3.0",
			"class" : "MU\\NewsModule\\MUNewsModule",
			"displayname" : "News",
			"url" : "news",
			"oldnames" : [
				"MUNews"
			],
			"capabilities" : {
				"user" : {
					"route" : "munewsmodule_message_index"
				},
				"admin" : {
					"route" : "munewsmodule_message_adminindex"
				},
				"categorizable" : {
					"entities" : [
						"MU\\NewsModule\\Entity\\MessageEntity"
					]
				}
			},
			"securityschema" : {
				"MUNewsModule::" : "::",
				"MUNewsModule:ItemListBlock:" : "Block title::",
				"MUNewsModule:ItemBlock:" : "Block title::",
				"MUNewsModule:ModerationBlock:" : "Block title::",
				"MUNewsModule:Message:" : "Message ID::",
				"MUNewsModule::Ajax" : "::"
			}
		}
	},
	"config" : {
		"vendor-dir" : "vendor",
		"preferred-install" : "dist",
		"optimize-autoloader" : true,
		"platform" : {
			"php" : "5.5.9"
		}
	}
}
