{
    "name": "mu/news-module",
    "version": "2.1.0",
    "description": "MUNewsModule generated by ModuleStudio 1.5.0.",
    "type": "zikula-module",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Michael Ueberschaer",
            "email": "info@homepages-mit-zikula.de",
            "homepage": "https://homepages-mit-zikula.de",
            "role": "owner"
        }
    ],
    "autoload": {
        "psr-4": { "MU\\NewsModule\\": "" }
    },
    "require": {
        "dompdf/dompdf": "^2",
        "php": ">=7.2.5"
    },
    "require-dev": {
    },
    "extra" : {
        "zikula" : {
            "core-compatibility": ">=3.0.0 <4.0.0",
            "class": "MU\\NewsModule\\MUNewsModule",
            "displayname": "News",
            "url": "news",
            "icon": "fas fa-database",
            "oldnames": ["MUNews"],
            "capabilities": {
                "admin": {
                    "route": "munewsmodule_message_adminindex"
                },
                "user": {
                    "route": "munewsmodule_message_index"
                },
                "categorizable": {
                    "entities": [
                        "MU\\NewsModule\\Entity\\MessageEntity"
                    ]
                }
            },
            "securityschema": {
                "MUNewsModule::": "::",
                "MUNewsModule:ItemListBlock:": "Block title::",
                "MUNewsModule:ItemBlock:": "Block title::",
                "MUNewsModule:ModerationBlock:": "Block title::",
                "MUNewsModule:Message:": "Message ID::",
                "MUNewsModule:Image:": "Image ID::",
                "MUNewsModule:Message:Image": "Message ID:Image ID:",
                "MUNewsModule::Ajax": "::"
            }
        }
    },
    "config": {
        "vendor-dir": "vendor",
        "platform": {
            "php": "7.2.5"
        },
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
